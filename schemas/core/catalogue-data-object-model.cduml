		sourceCode : string
		signature ?: string
	}

	package HypertyDescriptor <<Rect>> {
	}

	package ProtoStubDescriptor <<Rect>> {
	}

	package HypertyRuntimeDescriptor <<Rect>> {
	}

	package HypertyInterceptorDescriptor <<Rect>> {
	}

	package DataObjectSchema <<Rect>> {
	}

	package IdpProxyDescriptor <<Rect>> {
		class IdpProxyDescriptor {	
		}
	}

	CatalogueDataObject -right-> SourcePackage
	CatalogueDataObject -left-> CatalogueObjectType
	CatalogueDataObject -up-> DataObjectSourceLanguage

	CatalogueDataObject.HypertyDescriptor.HypertyDescriptor .up.|> CatalogueDataObject
	CatalogueDataObject.ProtoStubDescriptor.ProtoStubDescriptor .up.|> CatalogueDataObject

	IdpProxyDescriptor.IdpProxyDescriptor .up.|> CatalogueDataObject.ProtoStubDescriptor.ProtoStubDescriptor

	CatalogueDataObject.HypertyRuntimeDescriptor.HypertyRuntimeDescriptor .up.|> CatalogueDataObject
	CatalogueDataObject.HypertyInterceptorDescriptor.HypertyInterceptorDescriptor .up.|> CatalogueDataObject
	CatalogueDataObject.DataObjectSchema.DataObjectSchema .up.|> CatalogueDataObject
}

@enduml
package CatalogueDataObject <<Rect>> {

	class CatalogueDataObject {
		cguid : string
		type : CatalogueObjectType
		version : string
		objectName : string
		description : string
		language : DataObjectSourceLanguage
		sourcePackageURL : URL.URL
		sourcePackage ?: SourcePackage
		signature ?: string
	}

	class CatalogueObjectType {
		HYPERTY
		PROTOSTUB
		IDP_PROXY

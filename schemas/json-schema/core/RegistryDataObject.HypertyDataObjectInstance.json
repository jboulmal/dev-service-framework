{
	"$schema": "http://json-schema.org/draft-04/schema#",

	"id": "HypertyDataObjectInstance",
	"type": "object",
	"required": ["owner", "name"],
	"anyOf": [
		{ "$ref": "#/HypertyContextDataObjectInstance" },
		{ "$ref": "#/HypertyCommunicationDataObjectInstance" }
	]
	,
	"properties": {
		"owner": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"tags": {
			"type": "array",
			"items": {
				"type": "string"
			}
		}
	}, 
	"HypertyCommunicationDataObjectInstance": {
		"type": "object",
		"required": ["url"],
		"properties": {
			"url": {
				"type": "string"
			}
		}
	}, 
	"HypertyContextDataObjectInstance": {
		"type": "object",
		"required": ["url"],
		"properties": {
			"url": {
				"type": "string"
			}
		}
	}
}

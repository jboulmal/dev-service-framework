{
	"$schema": "http://json-schema.org/draft-04/schema#",

	"id": "URL",
	"type": "object",
	"required": ["scheme"],
	"anyOf": [
		{ "type": "string" },
		{ "type": "string" },
		{ "type": "string" },
		{ "type": "string" },
		{ "type": "string" },
		{ "type": "string" },
		{ "type": "string" },
		{ "type": "string" },
		{ "type": "string" },
		{ "type": "string" }
	]
	,
	"properties": {
		"scheme": {
			"type": "string"
		},
		"username": {
			"type": "string"
		},
		"password": {
			"type": "string"
		},
		"host": {
			"type": "string"
		},
		"port": {
			"type": "string"
		},
		"path": {
			"type": "string"
		},
		"query": {
			"type": "string"
		},
		"fragment": {
			"type": "string"
		}
	}, 
	"UserURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "user"
			}
		}
	}, 
	"UserGUIDURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "user-guid"
			}
		}
	}, 
	"DomainURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "domain"
			}
		}
	}, 
	"UserAccountURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "acct"
			}
		}
	}, 
	"HypertyURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "hyperty"
			}
		}
	}, 
	"HypertyCatalogueURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "hyperty-catalogue"
			}
		}
	}, 
	"HypertyRuntimeURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "hyperty-runtime"
			}
		}
	}, 
	"CommunicationURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "comm"
			}
		}
	}, 
	"ConnectionURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "connection"
			}
		}
	}, 
	"ContextURL": {
		"type": "object",
		"required": ["scheme"],
		"properties": {
			"scheme": {
				"constant": "ctxt"
			}
		}
	}, 
	"HypertyCatalogueURLList": {
		"type": "object",
		"properties": {
		}
	}
}

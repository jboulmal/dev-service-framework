{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "IceCandidates",
	"type": "object",
	"required": ["status", "rTCIceConnectionState"],
	"additionalProperties": false,
	"properties": {
		"status": {
			"enum": [
				"new",
				"checking",
				"connected",
				"completed",
				"failed",
				"disconnected",
				"closed"
			]
		}
		,
		"rTCIceConnectionState": {
			"enum": [
				"new",
				"checking",
				"connected",
				"completed",
				"failed",
				"disconnected",
				"closed"
			]
		},
		"iceCandidateArray": {
			"type": "array",
			"items": {
				"$ref": "../Connection/IceCandidate.json"
			}
		}
	}
}

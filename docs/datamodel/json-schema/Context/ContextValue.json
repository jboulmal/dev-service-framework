{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "ContextValue",
	"type": "object",
	"required": ["name", "units", "value", "contextUnit", "basic"],
	"additionalProperties": false,
	"allOf": [
		{"$ref": "../RethinkType/ListElement.json"}
	],
	"properties": {
		"name": {
			"type": "string"
		},
		"units": {
			"$ref": "../Context/ContextUnit.json"
		},
		"value": {
			"enum": [
				"STRING",
				"BOOLEAN",
				"FLOAT",
				"DOUBLE"
			]
		},
		"sum": {
			"type": "null"
		},
		"time": {
			"$ref": "../RethinkType/Date.json"
		},
		"updateTime": {
			"$ref": "../RethinkType/Date.json"
		},
		"expires": {
			"$ref": "../RethinkType/Date.json"
		},
		"address": {
			"$ref": "../URL/HypertyRuntimeURL.json"
		}
		,
		"contextUnit": {
			"$ref": "../Context/ContextUnit.json"
		},
		"basic": {
			"enum": [
				"STRING",
				"BOOLEAN",
				"FLOAT",
				"DOUBLE"
			]
		}
		,
		"contextValueListInv": {
			"$ref": "../Context/ContextValueList.json"
		}
	}
}

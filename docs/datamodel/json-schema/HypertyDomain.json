{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "HypertyDomain",
	"type": "object",
	"required": ["identifier", "name", "governance", "infrastructure"],
	"additionalProperties": false,
	"properties": {
		"identifier": {
			"type": "string"
		},
		"name": {
			"$ref": "../URL/DomainURL.json"
		},
		"description": {
			"type": "string"
		}
		,
		"governance": {
			"$ref": "../Governance.json"
		},
		"infrastructure": {
			"$ref": "../Infrastructure.json"
		}
	}
}

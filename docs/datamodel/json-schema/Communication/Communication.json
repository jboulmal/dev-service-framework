{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "Communication",
	"type": "object",
	"required": ["id", "host", "owner", "startingTime", "lastModified", "duration", "communicationStatus"],
	"additionalProperties": false,
	"properties": {
		"id": {
			"type": "null"
		},
		"host": {
			"type": "null"
		},
		"owner": {
			"type": "null"
		},
		"startingTime": {
			"type": "null"
		},
		"lastModified": {
			"type": "null"
		},
		"duration": {
			"type": "null"
		}
		,
		"communicationStatus": {
			"enum": [
				"OPEN",
				"PENDING",
				"CLOSED",
				"PAUSED",
				"FAILED"
			]
		},
		"participantArray": {
			"type": "array",
			"items": {
				"$ref": "../Communication/Participant.json"
			}
		},
		"communicationQualityArray": {
			"type": "array",
			"items": {
				"$ref": "../Communication/CommunicationQuality.json"
			}
		},
		"messageArray": {
			"type": "array",
			"items": {
				"$ref": "../Message/Message.json"
			}
		}
	}
}
